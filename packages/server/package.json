{
  "name": "@oktaman/server",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc && cp -r src/app/brain/prompts dist/app/brain/ && cp -r src/app/brain/tools dist/app/brain/ && cp -r src/app/brain/skills dist/app/brain/ && cp -r src/assets dist/ && find src -name '*.mjs' -exec sh -c 'cp \"$1\" \"dist/${1#src/}\"' _ {} \\;",
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
